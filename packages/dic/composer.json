{
    "name": "outboardphp/di",
    "description": "Dependency Injection Container for the Outboard framework and anyone else who wants to use it",
    "license": "MPL-2.0",
    "type": "library",
    "keywords": [
        "php",
        "dic",
        "ioc",
        "container",
        "inversion of control",
        "dependency injection",
        "dice"
    ],
    "authors": [
        {
            "name": "Garrett Whitehorn",
            "homepage": "https://garrettw.net/",
            "role": "Lead Developer"
        }
    ],
    "homepage": "https://github.com/outboardphp/di",
    "support": {
        "issues": "https://github.com/outboardphp/outboard/issues"
    },
    "require": {
        "php": ">=8.4",
        "psr/container": "^2.0",
        "technically/callable-reflection": "^0.4.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "dg/bypass-finals": "^1.9",
        "ergebnis/composer-normalize": "^2.48",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.8",
        "phpcompatibility/php-compatibility": "@dev",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-mockery": "^2.0",
        "squizlabs/php_codesniffer": "^3.13.2"
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Outboard\\Di\\": "src/",
            "Outboard\\Di\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "minimum-stability": "dev",
        "prefer-stable": true,
        "sort-packages": true
    }
}
